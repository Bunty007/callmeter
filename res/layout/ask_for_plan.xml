<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="wrap_content"
	android:minWidth="200dip">
	<LinearLayout android:orientation="vertical"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:padding="5dip">
		<Button android:id="@+id/btn00" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan00"
			android:visibility="gone" />
		<Button android:id="@+id/btn01" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan01" />
		<Button android:id="@+id/btn02" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan02"
			android:visibility="gone" />
		<Button android:id="@+id/btn03" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan03"
			android:visibility="gone" />
		<Button android:id="@+id/btn04" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan04"
			android:visibility="gone" />
		<Button android:id="@+id/btn05" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan05"
			android:visibility="gone" />
		<Button android:id="@+id/btn06" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan06"
			android:visibility="gone" />
		<Button android:id="@+id/btn07" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan07"
			android:visibility="gone" />
		<Button android:id="@+id/btn08" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan08"
			android:visibility="gone" />
		<Button android:id="@+id/btn09" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="plan09"
			android:visibility="gone" />
		<Button android:id="@+id/cancel" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="@android:string/cancel"
			android:layout_marginTop="15dip" />
		<CheckBox android:id="@+id/set_default" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/set_selected_as_default_" />
		<TextView android:id="@+id/autohide" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/autohide_in_" />
	</LinearLayout>
</ScrollView>
